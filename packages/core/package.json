{
  "name": "@myancommerce/core",
  "version": "1.0.0",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "gulp:watch": "gulp -f ./build/gulpfile.ts watch",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "run:server": "node -r ts-node/register src/main.ts",
    "start:prod": "node dist/main",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc ./tsconfig.build.json --watch",
    "test": "jest",
    "test:watch": "jests --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "myancommerce"
  ],
  "dependencies": {
    "@apollo/gateway": "^0.44.1",
    "@myancommerce/common": "workspace:*",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/typeorm": "^8.0.2",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "chalk": "4.1.2",
    "dotenv": "^10.0.0",
    "graphql": "^15",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "ts-morph": "^13.0.2",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.41",
    "yn": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "description": "",
  "author": "Thant Yar Zar Hein",
  "ts-node": {
    "transpileOnly": true
  }
}
