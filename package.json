{
    "name": "myancommerce",
    "version": "1.0.0",
    "author": "Thant Yar Zar Hein",
    "description": "",
    "license": "MIT",
    "private": true,
    "scripts": {
        "lint": "eslint **/*.{js,ts}",
        "lint:fix": "pnpm run lint -- --fix",
        "test": "jest",
        "test:gen-snap": "jest --updateSnapshot",
        "test:tsc": "tsc --noEmit",
        "test:doc": "prettier --check \"**/*.md\"",
        "format": "lint-staged",
        "watch": "tsc --build --watch --verbose",
        "start:dev": "concurrently \"nx serve\" \"pnpm codegen -w\"",
        "build:packs": "tsc --build --force --verbose",
        "changeset": "changeset",
        "release": "changeset publish",
        "clear": "rimraf packages/*/{lib,tsconfig.tsbuildinfo,node_modules} && rimraf node_modules",
        "cz": "git-cz",
        "codegen": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@apollo/gateway": "0.46.0",
        "@nestjs/common": "8.2.6",
        "@nestjs/core": "8.2.6",
        "@nestjs/graphql": "9.1.2",
        "@nestjs/platform-express": "8.2.6",
        "@nestjs/typeorm": "8.0.3",
        "apollo-server-express": "3.6.2",
        "chalk": "4.1.2",
        "dotenv": "16.0.0",
        "graphql": "15.8.0",
        "pg": "8.7.1",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.2",
        "ts-morph": "13.0.3",
        "tslib": "2.3.1",
        "typeorm": "0.2.41",
        "yn": "5.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.20.0",
        "@commitlint/cli": "^16.1.0",
        "@commitlint/config-conventional": "^16.0.0",
        "@graphql-codegen/cli": "2.5.0",
        "@graphql-codegen/introspection": "2.1.1",
        "@graphql-codegen/typescript": "2.4.3",
        "@graphql-codegen/typescript-compatibility": "^2.1.5",
        "@graphql-codegen/typescript-document-nodes": "2.2.3",
        "@graphql-codegen/typescript-operations": "^2.2.4",
        "@graphql-codegen/typescript-resolvers": "2.5.0",
        "@nestjs/schematics": "^8.0.5",
        "@nestjs/testing": "^8.2.6",
        "@nrwl/cli": "13.7.2",
        "@nrwl/eslint-plugin-nx": "13.7.2",
        "@nrwl/jest": "13.7.2",
        "@nrwl/js": "^13.7.2",
        "@nrwl/linter": "13.7.2",
        "@nrwl/nest": "13.7.2",
        "@nrwl/node": "13.7.2",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/tao": "13.7.2",
        "@nrwl/workspace": "13.7.2",
        "@types/chance": "^1.1.3",
        "@types/jest": "^27.4.0",
        "@types/pg": "^8.6.4",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "chance": "^1.1.8",
        "concurrently": "^6.5.1",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "git-cz": "^4.8.0",
        "graphql-tag": "^2.12.6",
        "graphql-tools": "^8.2.0",
        "jest": "^27.4.7",
        "lint-staged": "^11.2.6",
        "node-fetch": "^3.2.0",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "run-script-webpack-plugin": "^0.0.11",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5",
        "webpack": "^5.68.0",
        "webpack-node-externals": "^3.0.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": false,
            "disableSubjectLowerCase": false,
            "maxHeaderWidth": 100,
            "maxLineWidth": 100,
            "defaultType": "",
            "defaultScope": "",
            "defaultSubject": "",
            "defaultBody": "",
            "defaultIssues": ""
        }
    }
}
