{
    "name": "myancommerce",
    "version": "1.0.0",
    "author": "Thant Yar Zar Hein",
    "description": "",
    "license": "MIT",
    "private": true,
    "scripts": {
        "lint": "eslint **/*.{js,ts}",
        "lint:fix": "pnpm run lint -- --fix",
        "test": "pnpm test:doc && pnpm test:lint && pnpm test:tsc && pnpm test:unit && pnpm test:pw",
        "test:unit": "jest --passWithNoTests",
        "test:tsc": "tsc --noEmit",
        "test:doc": "prettier --check \"**/*.md\"",
        "format": "lint-staged",
        "watch": "tsc --build --watch --verbose",
        "start": "concurrently -n watch \"pnpm watch\"",
        "build:packs": "tsc --build --force --verbose",
        "changeset": "changeset",
        "release": "changeset publish",
        "clear": "rimraf packages/*/{lib,tsconfig.tsbuildinfo,node_modules} && rimraf node_modules",
        "cz": "git-cz"
    },
    "dependencies": {
        "@apollo/gateway": "^0.44.1",
        "@nestjs/common": "7.6.18",
        "@nestjs/core": "7.6.18",
        "@nestjs/graphql": "9.1.2",
        "@nestjs/platform-express": "7.6.18",
        "@nestjs/typeorm": "8.0.2",
        "apollo-server-express": "^3.5.0",
        "chalk": "4.1.2",
        "dotenv": "14.2.0",
        "graphql": "15.8.0",
        "pg": "8.7.1",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.2",
        "ts-morph": "13.0.2",
        "tslib": "2.3.1",
        "typeorm": "0.2.41",
        "yn": "5.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "2.19.0",
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-conventional": "12.1.4",
        "@graphql-codegen/cli": "2.3.1",
        "@nestjs/schematics": "8.0.5",
        "@nestjs/testing": "8.2.5",
        "@nrwl/cli": "13.4.4",
        "@nrwl/eslint-plugin-nx": "13.4.4",
        "@nrwl/jest": "13.4.4",
        "@nrwl/js": "13.4.4",
        "@nrwl/linter": "13.4.4",
        "@nrwl/nest": "13.4.4",
        "@nrwl/node": "13.4.4",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/tao": "13.4.5",
        "@nrwl/workspace": "13.4.5",
        "@types/jest": "27.4.0",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "concurrently": "6.5.1",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-promise": "4.3.1",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "eslint-plugin-tsdoc": "0.2.14",
        "git-cz": "4.8.0",
        "jest": "26.6.3",
        "lint-staged": "12.1.7",
        "prettier": "2.5.1",
        "rimraf": "3.0.2",
        "run-script-webpack-plugin": "0.0.11",
        "ts-jest": "27.1.3",
        "typescript": "4.5.4",
        "webpack": "5.66.0",
        "webpack-node-externals": "3.0.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": false,
            "disableSubjectLowerCase": false,
            "maxHeaderWidth": 100,
            "maxLineWidth": 100,
            "defaultType": "",
            "defaultScope": "",
            "defaultSubject": "",
            "defaultBody": "",
            "defaultIssues": ""
        }
    }
