{
    "name": "myancommerce",
    "version": "1.0.0",
    "author": "Thant Yar Zar Hein",
    "description": "",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "nx build",
        "lint": "eslint **/*.{js,ts}",
        "lint:fix": "pnpm run lint -- --fix",
        "pretest": "pnpm db:restart && dotenv -e apps/core/.env.test -- pnpx prisma migrate dev --schema apps/core/prisma/schema.prisma",
        "test": "dotenv -e apps/core/.env.test -- jest -i --no-cache --watch ",
        "test:gen-snap": "jest --updateSnapshot",
        "test:tsc": "tsc --noEmit",
        "test:doc": "prettier --check \"**/*.md\"",
        "db:restart": "docker compose down && docker compose up -d && sleep 1",
        "prisma:migrate": "dotenv -e apps/core/.env.dev -- pnpx prisma migrate dev --schema apps/core/prisma/schema.prisma",
        "prisma:generate": "pnpx prisma generate --schema apps/core/prisma/schema.prisma",
        "prisma:format": "pnpx prisma format --schema apps/core/prisma/schema.prisma",
        "format": "lint-staged",
        "watch": "tsc --build --watch --verbose",
        "start:dev": "cross-env NODE_ENV=production concurrently \"nx serve\" \"pnpm codegen -w\"",
        "build:packs": "tsc --build --force --verbose",
        "changeset": "changeset",
        "release": "changeset publish",
        "codegen": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@apollo/gateway": "0.46.0",
        "@nestjs/common": "8.2.6",
        "@nestjs/config": "^1.2.0",
        "@nestjs/core": "8.2.6",
        "@nestjs/graphql": "9.1.2",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "8.4.0",
        "@nestjs/typeorm": "8.0.3",
        "@prisma/client": "^3.9.1",
        "apollo-server-express": "3.6.2",
        "bcrypt": "^5.0.1",
        "cache-manager": "^3.6.0",
        "cache-manager-redis-store": "^2.0.0",
        "chalk": "4.1.2",
        "dotenv": "16.0.0",
        "dotenv-cli": "^5.0.0",
        "express-session": "^1.17.2",
        "graphql": "15.8.0",
        "node-redis": "^0.1.7",
        "nx": "13.9.0",
        "passport": "^0.5.2",
        "passport-google-oauth2": "^0.2.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "8.7.3",
        "redis": "^4",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.2",
        "ts-morph": "13.0.3",
        "tslib": "2.3.1",
        "yn": "5.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.21.1",
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@graphql-codegen/cli": "2.6.2",
        "@graphql-codegen/introspection": "2.1.1",
        "@graphql-codegen/typescript": "2.4.5",
        "@graphql-codegen/typescript-compatibility": "^2.1.5",
        "@graphql-codegen/typescript-document-nodes": "2.2.5",
        "@graphql-codegen/typescript-operations": "^2.3.2",
        "@graphql-codegen/typescript-resolvers": "2.5.2",
        "@nestjs/schematics": "^8.0.8",
        "@nestjs/testing": "^8.4.0",
        "@nrwl/cli": "13.8.3",
        "@nrwl/eslint-plugin-nx": "13.8.3",
        "@nrwl/jest": "13.8.3",
        "@nrwl/js": "^13.8.3",
        "@nrwl/linter": "13.8.3",
        "@nrwl/nest": "13.8.3",
        "@nrwl/node": "13.8.3",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/tao": "13.8.3",
        "@nrwl/workspace": "13.8.3",
        "@types/bcrypt": "^5.0.0",
        "@types/chance": "^1.1.3",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^27.4.1",
        "@types/passport": "^1.0.7",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "apollo-server": "^3.6.3",
        "chance": "^1.1.8",
        "child_process": "^1.0.2",
        "concurrently": "^6.5.1",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "get-port": "^6.1.2",
        "git-cz": "^4.8.0",
        "graphql-request": "^4.0.0",
        "graphql-tag": "^2.12.6",
        "graphql-tools": "^8.2.0",
        "i": "^0.3.7",
        "jest": "^27.5.1",
        "lint-staged": "^11.2.6",
        "node-fetch": "3",
        "prettier": "^2.5.1",
        "prisma": "^3.10.0",
        "prisma-dbml-generator": "^0.8.3",
        "run-script-webpack-plugin": "^0.0.11",
        "ts-jest": "^27.1.3",
        "typescript": "^4.6.2",
        "webpack": "^5.70.0",
        "webpack-node-externals": "^3.0.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": false,
            "disableSubjectLowerCase": false,
            "maxHeaderWidth": 100,
            "maxLineWidth": 100,
            "defaultType": "",
            "defaultScope": "",
            "defaultSubject": "",
            "defaultBody": "",
            "defaultIssues": ""
        }
    }
}
